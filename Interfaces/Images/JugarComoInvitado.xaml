<Window x:Class="AhorcadoCliente.Interfaces.Images.JugarComoInvitado"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AhorcadoCliente.Interfaces.Images"
        xmlns:p ="clr-namespace:AhorcadoCliente.Properties"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="JugarComoInvitado" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FF24444D" Offset="1"/>
                <GradientStop Color="#FF5F448F"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Label x:Name="lbJugarInvitado" Content="{x:Static p:Resources.PlayAsGuess}" HorizontalAlignment="Left" Margin="100,34,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="18" Foreground="White"/>
    </Grid>
</Window>
